  "scripts": {
    "start": "react-app-rewired start --region jdiot",
    "build": "react-app-rewired build --region jdiot",
    "startJDC": "react-app-rewired start --region jdcloud",
    "buildJDC": "react-app-rewired build --region jdcloud",
    "startJC": "react-app-rewired start --region jichang",
    "buildJC": "react-app-rewired build --region jichang",
    "startLS": "react-app-rewired start --region lishui",
    "buildLS": "react-app-rewired build --region lishui",
    "startYQ": "react-app-rewired start --region yueqing",
    "buildYQ": "react-app-rewired build --region yueqing",
    "startYQCITY": "react-app-rewired start --region yqcity",
    "buildYQCITY": "react-app-rewired build --region yqcity",
    "startHF": "react-app-rewired start --region hefei",
    "buildHF": "react-app-rewired build --region hefei",
    "startJX": "react-app-rewired start --region jiaxing",
    "buildJX": "react-app-rewired build --region jiaxing",
    "startPS": "react-app-rewired start --region panshi",
    "buildPS": "react-app-rewired build --region panshi",
    "startMicro": "react-app-rewired start --isMicro true --isSeparate true",
    "buildMicro": "react-app-rewired build --isMicro true --isSeparate true",
    "trans": "react-app-rewired build && node_modules/.bin/atool-l10n",
    "bundleVisualize": "BUNDLE_VISUALIZE=1 react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "prettier/@typescript-eslint",
      "plugin:prettier/recommended"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true
        }
      ],
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "warn"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "IE 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "IE 11"
    ]
  },
  "devDependencies": {
    "@hot-loader/react-dom": "^16.13.0",
    "atool-l10n": "^0.4.0",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-react-intl": "^7.5.28",
    "commander": "^6.1.0",
    "compression-webpack-plugin": "^4.0.0",
    "customize-cra": "^1.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "html-webpack-plugin": "4",
    "http-proxy-middleware": "^1.0.6",
    "less-loader": "^6.1.1",
    "lodash-webpack-plugin": "^0.11.5",
    "mini-css-extract-plugin": "^0.9.0",
    "minio": "^7.0.18",
    "prettier": "^2.0.5",
    "react-app-rewire-hot-loader": "^2.0.1",
    "react-app-rewired": "^2.1.6",
    "react-hot-loader": "^4.12.21",
    "react-xml-parser": "^1.1.8",
    "webpack-build-notifier": "^2.0.1",
    "webpack-bundle-analyzer": "^3.8.0"
  }
}
